@using WebApplication2.Models
@model Customer 
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CustomerView</title>
</head>
<body>
    <h1>@Model.CustomerName,@((string)ViewData["greeting"])</h1> 
    <div> 
        <h2>客户信息</h2>
        <p>姓名：@Model.CustomerName</p>
        <p>地址：@Model.Address</p>
    </div>
</body>
</html>
